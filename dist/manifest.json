{
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "name": "Squid - Buy Button",
  "description": "Buy NFTs with Squid",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.js-loader-6fd7d3b6.js"
      ],
      "matches": [
        "https://*.opensea.io/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.opensea.io/*"
      ],
      "resources": [
        "assets/content.js-6437c6ef.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
