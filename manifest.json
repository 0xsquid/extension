{
  "manifest_version": 3,
  "permissions": ["storage"],
  "name": "Squid - Buy Button",
  "description": "Buy NFTs with Squid",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://*:*/*"]
    }
  ]
}
